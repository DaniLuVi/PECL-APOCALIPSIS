<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="288.0" prefWidth="583.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.trabajofinal.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox prefHeight="242.0" prefWidth="535.0">
      <children>
      
          <Label fx:id="welcomeText" prefHeight="26.0" prefWidth="286.0" text="APOCALIPSIS ZOMBIE" textAlignment="RIGHT">
            <font>
               <Font size="14.0" />
            </font>
            <opaqueInsets>
               <Insets bottom="100.0" top="1000.0" />
            </opaqueInsets>
            <VBox.margin>
               <Insets bottom="20.0" left="200.0" />
            </VBox.margin>
         </Label>
          <ImageView fitHeight="157.0" fitWidth="359.0" preserveRatio="true">
            <image>
               <Image url="@../../../images/apocalipsis.jpg" />
            </image></ImageView>
          <Button onAction="#iniciarJuego" text="Iniciar juego" textAlignment="CENTER">
            <opaqueInsets>
               <Insets top="2000.0" />
            </opaqueInsets>
            <VBox.margin>
               <Insets left="230.0" top="20.0" />
            </VBox.margin>
         </Button>
      </children>
   </VBox>
</VBox>
